<g:VerticalPanel ui:field="vp" >
                 <g:HorizontalPanel >		
				 	<g:VerticalPanel styleName="{style.header}">
				 	<g:HTML><h3>Cloud GIS</h3></g:HTML></g:VerticalPanel>
					<g:VerticalPanel styleName="{style.logio}">
					
				  	<g:HTML><h5>Login/out Panel</h5></g:HTML>
				  	</g:VerticalPanel>
				 </g:HorizontalPanel>
				  
  <g:HorizontalPanel ui:field="hp">
	<g:VerticalPanel styleName="{style.queries}" ui:field="vp1">
<!--			 <g:VerticalPanel styleName="{style.queries}">-->
			 <g:HTML><h4>Site Search</h4></g:HTML>
			 <g:TabLayoutPanel barUnit='PX' barHeight='20' styleName="{style.tabPanel}"  ui:field="queryTabPanel" >	
				<g:tab>
					<g:customHeader size='5'>
          			<g:Label styleName="{style.label}">LatLong </g:Label>
        			</g:customHeader>
				    	<g:VerticalPanel ui:field="vpLatLng" styleName="{style.vLatLongPanel}" >
						<g:Label ui:field="lblLatitude" text="P1 Latitude" styleName="{style.textlabel}" />
						<g:TextBox ui:field="tbLatitude" styleName="{style.lattextbox1}" text="Enter Lattitude"></g:TextBox>
						<g:Label ui:field="lblLongitude" text="P1 Longitude" styleName="{style.textlabel}" />
						<g:TextBox ui:field="tbLongitude" styleName="{style.lattextbox1}" text="Enter Longitude"></g:TextBox>
						<g:Label ui:field="lblLatLngRadius" text="Specified Radius" styleName="{style.textlabel}"/>
						<g:TextBox ui:field="tbLatLngRadius" styleName="{style.lattextbox2}" text="Enter Radius" />
						<g:HorizontalPanel ui:field="hpLatLng">
							<g:Button ui:field="btnLatLngSearch" text="Search" styleName="{style.Button}" />
							<g:Button ui:field="btnLatLngClear" text="Clear" styleName="{style.Button}" />
						</g:HorizontalPanel>
						<g:Label ui:field="lblError"></g:Label>
					</g:VerticalPanel>
	
				</g:tab>
				<g:tab>
					<g:customHeader size='5'>
          			<g:Label styleName="{style.label}">Attribute</g:Label>
        			</g:customHeader>    	
				    	<g:VerticalPanel ui:field="vpAttribute" styleName="{style.vLatLongPanel}" >
						<g:Label ui:field="lblAttribute" text="Address :" styleName="{style.textlabel}"/>
						<g:TextBox ui:field="tbAttribute" styleName="{style.lattextbox1}" text="Enter Address"></g:TextBox>
						<g:Label ui:field="lblAttributeRadius" text="Specified Radius" styleName="{style.textlabel}"/>
						<g:TextBox ui:field="tbAttributeRadius" styleName="{style.lattextbox2}" text="Enter Radius"></g:TextBox>
						<g:HorizontalPanel ui:field="hpAttribute">
							<g:Button ui:field="btnAttributeSearch" text="Search" styleName="{style.Button}" />
							<g:Button ui:field="btnAttributeClear" text="Clear" styleName="{style.Button}" />
						</g:HorizontalPanel>
						</g:VerticalPanel>
				</g:tab>
				<g:tab>
					<g:customHeader size='5'>
          			<g:Label styleName="{style.label}">Spatial</g:Label>
        			</g:customHeader>
						<g:VerticalPanel ui:field="vpSpatial" styleName="{style.vLatLongPanel}" >
						<g:Label ui:field="lblState" text="State:" styleName="{style.textlabel}"/>
						<g:ListBox ui:field="lbState" />
						<g:Label ui:field="districtLabel" text="District:" styleName="{style.textlabel}"/>
						<g:ListBox ui:field="lbDistrict" />
						<g:Label ui:field="lblLocalBody" text="Local Body:" styleName="{style.textlabel}"/>
						<g:ListBox ui:field="lbLocalBody" />
						<g:Label ui:field="lblSpatialRadius" text="Specified Radius" styleName="{style.textlabel}"/>
						<g:TextBox ui:field="tbSpatialRadius" styleName="{style.lattextbox2}" text="Enter Radius"></g:TextBox>
							<g:HorizontalPanel ui:field="hpSpatial">
							<g:Button ui:field="btnSpatialSearch" text="Search" styleName="{style.Button}" />
							<g:Button ui:field="btnSpatialClear" text="Clear" styleName="{style.Button}" />
						</g:HorizontalPanel>
						</g:VerticalPanel>
				</g:tab>
				<g:tab>
					<g:customHeader size='5'>
    			    <g:Label styleName="{style.label}">GeoCoded</g:Label>
        			</g:customHeader>
        				<g:VerticalPanel ui:field="vpGeoCoded" styleName="{style.vLatLongPanel}" >
						<g:Label ui:field="lblGeoCodedAddress" text="Address:" styleName="{style.textlabel}"/>
						<g:TextBox ui:field="tbGeoCodedAddress" text="Enter Address" />
						<g:Label ui:field="lblGeoCodedRadius" text="Specified Radius:" styleName="{style.textlabel}"/>
						<g:TextBox  ui:field="tbGeoCodedRadius"  text="Enter Radius"/>
							<g:HorizontalPanel ui:field="hpGeoCoded">
							<g:Button ui:field="btnGeoCodedSearch" text="Search" styleName="{style.Button}" />
							<g:Button ui:field="btnGeoCodedClear" text="Clear" styleName="{style.Button}" />
						</g:HorizontalPanel>
						</g:VerticalPanel>
				</g:tab>
		</g:TabLayoutPanel>	
<!--	</g:VerticalPanel>		-->
				<g:VerticalPanel ui:field="vpAdmin"  styleName="{style.adminp}">
					<g:HTML><h4>Admin Panel</h4></g:HTML>
					<g:FormPanel ui:field="fpAdmin" />
					<g:HorizontalPanel ui:field="hpAdmin">
<!--					<g:Button ui:field="submitButton" text="Submit" styleName="{style.Button}" />-->
<!--					</g:HorizontalPanel>-->
<!--					<g:HorizontalPanel>-->
					<g:Button ui:field="btnAdminLoad" text="Load" styleName="{style.Button}" />
					<g:Button ui:field="btnAdminDisplay" text="Display" styleName="{style.Button}" />
					</g:HorizontalPanel>
				</g:VerticalPanel>
		</g:VerticalPanel>
					
					<g:VerticalPanel >
						<g:HorizontalPanel styleName="{style.tools}">
						<g:HTML><h5>Tools Panel</h5></g:HTML> 
						</g:HorizontalPanel>
						<g:VerticalPanel styleName="{style.map}" ui:field="mapwrapper">	
							<gw:MapWidget ui:field="map">
							</gw:MapWidget>
						</g:VerticalPanel>
					</g:VerticalPanel>

					<g:VerticalPanel styleName="{style.layerm}" ui:field="vp2">
					<g:HTML><h4>Layer Manager Panel</h4>
					</g:HTML>
				
					
					</g:VerticalPanel>
			</g:HorizontalPanel>
<!--			-->
			
			<g:VerticalPanel ui:field="vpTable" styleName="{style.datag}">
				<g:VerticalPanel ui:field="vpDataGird">
				</g:VerticalPanel>
				<g:VerticalPanel>
					<g:Label ui:field="lblMessages"></g:Label>
					<g:HorizontalPanel ui:field="hp1"  >
	      	  			<g:HorizontalPanel ui:field="hpTable" >
				     	    <g:TextBox ui:field="tbFilter"></g:TextBox>
				     	    <g:Button ui:field="btnApplyFilter"  text="Apply Filter" styleName="{style.Button}"></g:Button>
				      	    <g:Button ui:field="btnClearFilter" text="Clear Filter" styleName="{style.Button}"></g:Button>
				      	    <g:Button ui:field="btnMarkAll" text="Mark All" styleName="{style.Button}"></g:Button>
				            <g:Button ui:field="btnMarkNothing" text="Mark Nothing" styleName="{style.Button}"></g:Button>
				            <g:Button ui:field="btnShowMarked" text="Show Marked" styleName="{style.Button}"></g:Button>
				           </g:HorizontalPanel>
	          		</g:HorizontalPanel>
	         	 
	        	 </g:VerticalPanel>
			</g:VerticalPanel>
        
        
